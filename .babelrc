{
    "presets": [
        [
            "@babel/env",
            {
                "modules": false
            }
        ],
        "@babel/flow",
        "@babel/react"
    ],
    "plugins": [
        "@babel/transform-runtime",
        "@babel/proposal-export-namespace-from",
        "@babel/syntax-dynamic-import",
        "@babel/proposal-class-properties"
    ]
}
