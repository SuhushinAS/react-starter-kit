{
    "globExcludePatterns": [
        "www/**",
        "flow-typed/**",
        "node_modules/**"
    ],
    "globIncludePatterns": [
        "src/**/*.js",
        "src/**/*.jsx"
    ],
    "outputDir": "coverage/flow",
    "threshold": 90,
    "reportTypes": [
        "html",
        "text"
    ]
}
